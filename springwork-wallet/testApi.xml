<?xml version="1.0" encoding="UTF-8"?>  
<wallet>
<trans transcode="000001" name="openAccount" delimiter="`">  
    <feild varname="userNo" serials="0" default="00001"/> 
    <feild varname="accNo" serials="2" default="10001"/> 
    <feild varname="accountType" serials="3" default="00"/> 
    <feild varname="SysRemark" serials="1" default="我是大爷"/> 
</trans> 

<trans transcode="000002" name="closeAccount" delimiter="`">  
    <feild varname="userNo" serials="0" default="00001"/> 
    <feild varname="accNo" serials="2" default="10001"/> 
    <feild varname="accountType" serials="3" default="00"/> 
    <feild varname="SysRemark" serials="1" default="我不是大爷"/> 
</trans> 
</wallet>